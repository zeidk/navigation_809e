<launch>
  <arg name="world_name" default="$(find navigation_809e)/world/usar.world" />
  <arg name="model" default="waffle" />


  <!-- Launch turtlebot3 bringup -->
  <include file="$(find navigation_809e)/launch/mapping/bringup.launch">
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="model" value="$(arg model)" />
    <arg name="rviz_config" value="$(find navigation_809e)/rviz/mapping.rviz" />
  </include>

  <!-- Start mapping -->
  <include file="$(find turtlebot3_slam)/launch/turtlebot3_gmapping.launch">
    <arg name="model" value="$(arg model)" />
  </include>
</launch>
